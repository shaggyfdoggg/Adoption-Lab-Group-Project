@model List<AdoptList>

<h1>Welcome to the Furever Adoption Agency.</h1>
<h2>Here are our animals</h2>

<div class="text-center">





    @{
        List<AdoptList> distinctBreeds = Model.DistinctBy(b => b.Breed).ToList();
        foreach (AdoptList a in distinctBreeds)
        {
            <a href="@Url.Action("ListOfAnimals")">
                <p>@a.Breed</p>
                List<Breed></Breed>
            </a>
        }
    }


